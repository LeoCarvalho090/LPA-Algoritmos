 ,
 Algoritmo "TAbuada"

Var
   Numero, ContVolta : inteiro


Inicio
      escreva("Digite um numero para expor a tabuada")
      leia(Numero)
      ContVolta <-1
      enquanto (contvolta <=10) faca
               escreval(Numero, " * ", Contvolta, " = ", Numero*ContVolta)
               contvolta <- contvolta + 1
      fimenquanto
      
Fimalgoritmo